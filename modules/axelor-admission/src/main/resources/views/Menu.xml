<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

	<menuitem title="Admission" name="admission-root" />

	<menuitem title="Admission Entry" name="admission-root-admission-entry" parent="admission-root" action="action.admission.view.admission.entry" />
	<action-view name="action.admission.view.admission.entry" title="Admission Entry" model="com.axelor.apps.admission.db.AdmissionEntry">
		<view type="grid" name="admission-entry-grid" />
		<view type="form" name="admission-entry-form" />
	</action-view>

	<menuitem title="College" name="admission-root-college" parent="admission-root" action="action.admission.view.college" />
	<action-view name="action.admission.view.college" title="College" model="com.axelor.apps.admission.db.College">
		<view type="grid" name="college-grid" />
		<view type="form" name="college-form" />
	</action-view>

	<menuitem title="Faculty" name="admission-root-faculty" parent="admission-root" action="action.admission.view.faculty" />
	<action-view name="action.admission.view.faculty" title="Faculty" model="com.axelor.apps.admission.db.Faculty">
		<view type="grid" name="faculty-grid" />
		<view type="form" name="faculty-form" />
	</action-view>

	<menuitem title="Admission Process" name="admission-root-admission-process" parent="admission-root" action="action.admission.view.admission.process" />
	<action-view name="action.admission.view.admission.process" title="Admission Process" model="com.axelor.apps.admission.db.AdmissionProcess">
		<view type="grid" name="admission-process-grid" />
		<view type="form" name="admission-process-form" />
	</action-view>


</object-views>
